  print ('number is not positive')
if x>=0 and x%2==0:
  print ('even')
if x>=0 and x%2!=0:
  print ('odd') 
x=25


lower, upper = 2, 25
for num in range (lower, upper + 1):
  flag = 0
  if num > 1:
    for i in range (2, num):
      if (num % i == 0):
        flag = 1
        break
    if flag == 0:
      print (num)
      
      
      def interval_intersect(a, b, c, d):
    return (c <= b) or (a <= d)

def test(a, b, c, d):
    "Intervals [" + str(a) + ", " + str(b) + "] and [" + str(c) + ", " + str(d) + "]",
    if interval_intersect(a, b, c, d):
      print ("True")
    else:
      print ("False")
